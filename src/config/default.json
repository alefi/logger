{
  "dateFormat": "DD.MM.YYYY HH:mm:ss.SSS",
  "exitOnError": false,
  "errorPropertiesToSerialize": "code, name, message, stack",
  "level": "info",
  "logFormat": "${ timestamp } ${ ctx.requestId } - ${ level } [${ label }] ${ message } ${ meta }",
  "prettyPrint": false,
  "timestamp": true,
  "transports": [
    "console"
  ],
  "useTimezone": false
}
